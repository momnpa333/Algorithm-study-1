#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#define INT_MAX 99
int ary[INT_MAX];
int ary2[INT_MAX];
void bisearch(int l,int r,int middle,int key);
int main()
{
   int search_num,total_num,i=0;
   FILE* fp;

   fp = fopen("input.txt", "r");
   fscanf(fp,"%d %d", &search_num, &total_num);

   for (int i = 0; i < search_num; i++)
   {
      fscanf(fp, "%d", &ary2[i]);
   }
   while (!feof(fp))
   {
      fscanf(fp, "%d", &ary[i++]);
   }

   for (int i = 0; i < search_num; i++)
   {
      bisearch(0, total_num, total_num/2,ary2[i]);
   }
   return 0;
}
void bisearch(int l,int r,int middle,int key)
{
   middle = (r + l) / 2;
   if (l >= r)
   {
      printf("no\n");
      return;
   }
   else if (key == ary[middle])
   {
      printf("yes\n");
      return;
   }
   else if (key < ary[middle])
   {
      r = middle;
      bisearch(l, r,middle, key);
      return;
   }
   else
   {
      l = middle;
      bisearch(l+1, r,middle, key);
      return;
   }
}
